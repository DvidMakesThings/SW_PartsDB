{
  "version": "2.0",
  "description": "Universal DMT classification for electronic parts with fixed-width codes and per-item sequence",
  "code_format": {
    "pattern": "TTFFCCSSXXX",
    "human_readable": "DMT-TTFFCCSSXXX",
    "segments": {
      "TT": "Top domain 00-99",
      "FF": "Family inside domain 00-99",
      "CC": "Class or subtype 00-99 (family specific)",
      "SS": "Style or vendor bucket 00-99 (family specific)",
      "XXX": "Per-item sequence 001-999 to ensure uniqueness per TTFFCCSS"
    },
    "render": "prefix 'DMT-' + concatenated digits"
  },
  "cross_cutting_class_codes": {
    "90": "Accessories",
    "91": "Kits",
    "92": "Evaluation or development items",
    "93": "Software or services",
    "94": "Assemblies or finished modules",
    "95": "Adapters",
    "96": "Cables that belong to this family",
    "97": "Tools specific to this family",
    "99": "Misc or specialized"
  },
